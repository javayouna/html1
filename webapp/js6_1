<script>
/* 
  응용문제: 해당 함수들을 이용하여 결과값을 return받는 코드를 작성하시오.
	숫자 20과 30을 함수로 전달
	결과값 출력 600이 출력되어야 합니다.
*/

function a(n,nn){
	 var result=b(n,nn);
	 //console.log(result);
}
function b(n,nn){
	 var cal= n*nn; 
	 	return cal;
}
a(20,30);

</script>
